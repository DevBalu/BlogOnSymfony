{% extends "::base.html.twig" %}

{% block title %}AppBundle:about:insertInfo ABOUT {% endblock %}

{% block body %}
<h1>Welcome to the about:insertInfo page</h1>
<div class="container">
    <div class="row">
        <form action="/saveAboutInfo" method="post" style="width: 100%;">
            <div class="col col-md-12">
                <div class="input-group input-group-lg">
                    <textarea name="img" type="text" class="form-control" placeholder="img"></textarea>
                </div>
            </div>
            <div class="col col-md-12">
                <div class="input-group input-group-lg">
                    <input name="nickname" type="text" class="form-control" placeholder="nickname" >
                </div>
            </div>
            <div class="col col-md-12">
                <div class="input-group input-group-lg">
                    <textarea name="globInfo" type="text" class="form-control" placeholder="globInfo"></textarea>
                </div>
            </div>
            <div class="col col-md-12">
                <div class="input-group input-group-lg">
                    <input name="contactInfo" type="text" class="form-control" placeholder="contactInfo">
                </div>
            </div>
            <div class="col col-md-12">
                <span class="input-group-btn">
                    <button type="submit" class="btn btn-secondary" type="button">add</button>
                </span>
            </div>

        </form>

    </div>
</div>
{% endblock %}
